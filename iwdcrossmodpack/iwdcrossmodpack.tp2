BEGIN ~Icewind Dale Crossmod Banter Pack~
  REQUIRE_PREDICATE (GAME_IS ~iwdee~) ~This component must be installed on IWD:EE.~

PRINT ~MINIMUM DELAY BETWEEN BANTERS

This option sets the absolute minimum amount of time between banters. 

Please choose one of the following:
[1] 5 minutes
[2] 10 minutes
[3] 15 minutes (recommended)
[4] 30 minutes
[5] 45 minutes
[6] 1 hour~

OUTER_SPRINT ~banttime~ ~placeholder_value~
OUTER_WHILE (!(IS_AN_INT ~banttime~) OR (~banttime~ > 0x6) OR (~banttime~ < 0x1)) BEGIN
  ACTION_READLN ~banttime~
END
      ACTION_IF ("banttime" = 1) THEN BEGIN
	  OUTER_SET banttime = 300
      PRINT ~Installing 5 minutes minimum delay between banters...~
      END
      ACTION_IF ("banttime" = 2) THEN BEGIN
	  OUTER_SET banttime = 600
      PRINT ~Installing 10 minutes minimum delay between banters...~
      END
      ACTION_IF ("banttime" = 3) THEN BEGIN
	  OUTER_SET banttime = 900
      PRINT ~Installing 15 minutes minimum delay between banters...~
	  END
      ACTION_IF ("banttime" = 4) THEN BEGIN
	  OUTER_SET banttime = 1800
      PRINT ~Installing 30 minutes minimum delay between banters...~
	  END
      ACTION_IF ("banttime" = 5) THEN BEGIN
	  OUTER_SET banttime = 2700
      PRINT ~Installing 45 minutes minimum delay between banters...~
	  END
      ACTION_IF ("banttime" = 6) THEN BEGIN
	  OUTER_SET banttime = 3600
      PRINT ~Installing 1 hour minimum delay between banters...~
	  END
/*
PRINT ~BANTER PROBABILITY

This option sets the probability of a party member attempting to initiate a banter between delays.

Please choose one of the following:
[1] 20% chance
[2] 40% chance (recommended)
[3] 60% chance
[4] 80% chance
[5] 100% chance~

OUTER_SPRINT ~bantprob~ ~placeholder_value~
OUTER_WHILE (!(IS_AN_INT ~bantprob~) OR (~bantprob~ > 0x5) OR (~bantprob~ < 0x1)) BEGIN
  ACTION_READLN ~bantprob~
END
      ACTION_IF ("bantprob" = 1) THEN BEGIN
	  OUTER_SET bantprob1 = 5
	  OUTER_SET bantprob2 = 80
      PRINT ~Installing 20% chance of banter between timers...~
      END
      ACTION_IF ("bantprob" = 2) THEN BEGIN
	  OUTER_SET bantprob1 = 10
	  OUTER_SET bantprob2 = 60
      PRINT ~Installing 40% chance of banter between timers...~
      END
      ACTION_IF ("bantprob" = 3) THEN BEGIN
	  OUTER_SET bantprob1 = 15
	  OUTER_SET bantprob2 = 40
      PRINT ~Installing 60% chance of banter between timers...~
	  END
      ACTION_IF ("bantprob" = 4) THEN BEGIN
	  OUTER_SET bantprob1 = 20
	  OUTER_SET bantprob2 = 20
      PRINT ~Installing 80% chance of banter between timers...~
	  END
      ACTION_IF ("bantprob" = 5) THEN BEGIN
	  OUTER_SET bantprob1 = 25
	  OUTER_SET bantprob2 = 0
      PRINT ~Installing 100% chance of banter between timers...~
	  END
*/
	  OUTER_SET bantprob1 = 25
	  OUTER_SET bantprob2 = 0
// Banter initiation script - hopefully I find a better way
	EXTEND_TOP ~BALDUR.BCS~ ~%MOD_FOLDER%/baf/baldur.baf~ EVAL
