//NELLA - DUSKY ROMANCE CONFLICT

CHAIN IF WEIGHT #-1 ~Global("C0DuskyNellaConflict","GLOBAL",2)~ THEN O#NELLA duskynella.conflict
@0
DO ~IncrementGlobal("C0DuskyNellaConflict","GLOBAL",1)~
==L#DUSKYJ @1
==O#NELLA @2
==L#DUSKYJ @3
==O#NELLA @4
==L#DUSKYJ @5
END
IF~~THEN REPLY @6 DO ~SetGlobal("O#NellaMatch","GLOBAL",0)~ EXTERN O#NELLA L#DUSKNELR-1
IF~~THEN REPLY @7 DO ~SetGlobal("DuskyRomanceActive","GLOBAL",3)~ EXTERN L#DUSKYJ L#DUSKNELR-2

CHAIN O#NELLA L#DUSKNELR-1
@8
==L#DUSKYJ @9
==L#DUSKYJ @10
DO ~~ EXIT

CHAIN L#DUSKYJ L#DUSKNELR-2
@11
==L#DUSKYJ @12
END
IF~~THEN REPLY @13 EXTERN L#DUSKYJ L#DUSKNELR-3
IF~~THEN REPLY @14 EXTERN L#DUSKYJ L#DUSKNELR-4

CHAIN L#DUSKYJ L#DUSKNELR-3
@15
==L#DUSKYJ @16
EXIT

CHAIN L#DUSKYJ L#DUSKNELR-4
@17
EXTERN L#DUSKYJ L#DUSKNELR-3

//TERI - DUSKY ROMANCE CONFLICT

CHAIN IF WEIGHT #-1 ~Global("C0DuskyTeriConflict","GLOBAL",2)~ THEN O#TERI duskyteri.conflict
@18
DO ~IncrementGlobal("C0DuskyTeriConflict","GLOBAL",1)~
==L#DUSKYJ @19
==O#TERI @20
==L#DUSKY @21
==O#TERI @22
==L#DUSKY @23
==O#TERI @24
==L#DUSKY @25
==O#TERI @26
==L#DUSKY @27
==O#TERI @28
END
IF~~THEN REPLY @29 DO ~SetGlobal("DuskyRomanceActive","GLOBAL",3)~ EXTERN L#DUSKYJ L#DUSKTERR-1
IF~~THEN REPLY @30 DO ~SetGlobal("O#TeriMatch","GLOBAL",0)~ EXTERN O#TERI L#DUSKTERR-2

CHAIN L#DUSKYJ L#DUSKTERR-1
@31
== O#TERI @32
== L#DUSKYJ @33
== L#DUSKYJ @34
== L#DUSKYJ @35
