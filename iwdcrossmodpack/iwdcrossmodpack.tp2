	APPEND ~interdia.2da~ ~O#HOLVIR BO#HOLVI BO#HOLVI~ UNLESS ~O#HOLVIR~
	APPEND ~interdia.2da~ ~O#KORIN BO#KORIN BO#KORIN~ UNLESS ~O#KORIN~
