	APPEND ~interdia.2da~ ~O#SEVERN BO#SEVER BO#SEVER~ UNLESS ~O#SEVER~
	APPEND ~interdia.2da~ ~O#TERI BO#TERI BO#TERI~ UNLESS ~O#TERI~
	APPEND ~interdia.2da~ ~C0MMIN BC0MMIN BC0MMIN~ UNLESS ~C0MMIN~
	APPEND ~interdia.2da~ ~C0KARIHI BC0KARIH BC0KARIH~ UNLESS ~C0KARIHI~
	APPEND ~interdia.2da~ ~L#DENDJ L#DENDJB L#DENDJB~ UNLESS ~L#DENDJ~
	APPEND ~interdia.2da~ ~L#DUSKY L#DUSKYB L#DUSKYB~ UNLESS ~L#DUSKY~
